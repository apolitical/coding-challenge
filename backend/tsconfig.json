{
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "dist",
    "target": "es2021",
    "module": "commonjs",
    "strict": true,
    "types": ["node", "express", "jest"],
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts"],
  "exclude": [
    ".credentials",
    ".husky",
    ".vscode",
    ".coverage",
    "dist",
    "monitoring",
    "node_modules",
    "templates",
    "test"
  ]
}
